{{define "modal_dialog"}}
<div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
        <div class="modal-body">
            <div class="c-blank-slate c-blank-slate-muted">
                <div class="bc-avatar">
                    <i class="if if-alert"></i>
                </div>
                <h4>
                    File does not exist anymore. Maybe someone else deleted this? Please reload the page.
                </h4>
            </div>
        </div>
        <div class="modal-footer">
            <button class="btn btn-link modal-close" data-dismiss="modal">OK</button>
        </div>
    </div>
</div>
{{end}}
