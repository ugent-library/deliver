{{define "page_title"}}Spaces{{end}}
{{define "body"}}
    <form action="{{.Ctx.URLPath "create_folder" "space_id" .Var.SpaceID}}" method="post">
        <input type="text" name="name">
        <button type="submit">Create</button>
    </form>
    <ul>
    {{range .Var.Folders}}
        <li>{{.Name}}</li>
    {{end}}
    </ul>
{{end}}