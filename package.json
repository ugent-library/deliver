    "generate-issue-test": "node ./cypress/scripts/generate-issue-test.mjs"
  },
  "precommit": "precommit",
  "devDependencies": {
    "cypress": "^13.6.3",
    "cypress-common": "github:ugent-library/cypress-common#3.16.1",
    "env-cmd": "^10.1.0",
    "js-precommit-hook": "github:ugent-library/js-precommit-hook",
    "laravel-mix": "^6.0.11",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "typescript": "^5.3.2",
    "uglify-js": "^3.14.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.2.3",
    "bootstrap.native": "5.0.6",
    "htmx.org": "^1.9.0"
  }
